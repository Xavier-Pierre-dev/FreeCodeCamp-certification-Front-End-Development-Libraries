(function(n){var r=function(t){var i=t,e=i.find(".js-empty-value-check-box"),u=i.find(".js-disagree-value-check-box"),r=i.find(".js-i-agree-checkbox"),o=i.find(".js-customer-message-holder"),s=i.find(".js-i-agree-holder"),h=i.find(".js-already-agreed-holder"),a=JSON.parse(i.find('[data-sf-role="optinout-data"]').val()),c=i.closest('[data-sf-role="form-container"]'),f=c.find(".js-country-field");if(r.length>0)r.on("click",function(){var t=n(this),i=t.prop("checked");u.prop("checked",!i)});var l=function(t){var i=c.find('input[name="skipPrefillOnCountryChange"]');if(!i||!(i.length>0)||!i[0].checked){var r=n(t).find("option:selected").val();jQuery.inArray(r,a)!==-1?v():y()}},v=function(){s.show();h.hide();o.hide();e.prop("checked",!1);u.prop("checked",!0);r.prop("checked",!1)},y=function(){s.hide();h.hide();o.hide();e.prop("checked",!0);u.prop("checked",!1);r.prop("checked",!1)};if(f.length>0){f.on("change",function(){l(this)});l(f)}},t=n(".js-opt-in-out-container");if(t&&!(t.length<1))for(var i=0;i<t.length;i++)new r(n(t[i]))})(jQuery)